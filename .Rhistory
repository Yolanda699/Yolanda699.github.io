library(ggplot2)
library(dplyr)
# Count the occurrences of each combination of Gender and Alcohol level
pie_data <- drug_or %>%
count(Gender, Alcohol) %>%
mutate(percentage = n / sum(n) * 100)
# Define custom colors
custom_colors <- c("Male" = "#106898", "Female" = "#615ea8")
# Create a pie chart with custom colors and faceting
ggplot(pie_data, aes(x = "", y = n, fill = Gender, label = paste0(round(percentage), "%"))) +
geom_bar(width = 1, stat = "identity") +
geom_text(aes(label = paste0(round(percentage), "% (", n, ")")), position = position_stack(vjust = 0.5), color = "white") +
coord_polar("y", start = 0) +
facet_wrap(~Alcohol) + # Facet by Alcohol level
labs(title = "Distribution of Alcohol Consumption by Gender") +
scale_fill_manual(values = custom_colors) + # Apply custom colors
theme_void()
